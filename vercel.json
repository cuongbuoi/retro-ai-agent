{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".output",
  "github": {
    "enabled": true,
    "silent": false
  },
  "cleanUrls": true,
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api/$1" }],
  "regions": ["sin1"],
  "functions": {
    "api/**": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
