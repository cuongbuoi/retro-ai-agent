{
  "version": 2,
  "buildCommand": "chmod +x build.sh && ./build.sh",
  "outputDirectory": ".output",
  "github": {
    "enabled": true,
    "silent": false
  },
  "cleanUrls": true,
  "routes": [
    { "src": "/_nuxt/(.*)", "dest": "/_nuxt/$1" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/" }
  ],
  "regions": ["sin1"]
}
